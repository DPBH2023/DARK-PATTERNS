{
  "manifest_version": 2,
  "name": "Dark Pattern Detector",
  "version": "0.0.1",
  "description": "It detects dark patterns in text",
  "browser_action": {
      "default_popup": "popup.html",
      "default_title": "Click for popup"
  },
  "permissions": [
      "storage",
      "activeTab"
  ],
  "background": {
      "scripts": ["background.js"],
      "persistent": false
  }
}
